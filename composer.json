{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.spatie.be"
        }
    ],
    "require": {
        "php": "^7.3|8.0",
        "ext-json": "*",
        "aws/aws-sdk-php-laravel": "^3.1",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.4",
        "james-heinrich/getid3": "^1.9",
        "kladislav/laravel-chunk-upload": "dev-master",
        "laravel/cashier": "^10.0",
        "laravel/framework": "^7.30.1",
        "laravel/horizon": "^4.0",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "nicolaslopezj/searchable": "^1.11",
        "pion/laravel-chunk-upload": "^1.4",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.3",
        "snipe/banbuilder": "^2.2",
        "spatie/laravel-mailcoach": "^2.0",
        "spatie/laravel-mailcoach-ses-feedback": "^2.0",
        "spatie/laravel-mailcoach-unlayer": "^1.0",
        "spatie/laravel-permission": "^3.0",
        "spatie/laravel-sluggable": "^2.5",
        "stripe/stripe-php": "^7.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.3",
        "barryvdh/laravel-ide-helper": "^2.4",
        "beyondcode/helo-laravel": "^1.0",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/telescope": "^3.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag mailcoach-assets --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
